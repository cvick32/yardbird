(declare-fun a () (Array Int Int))
(declare-fun a_next () (Array Int Int))
(define-fun .a () (Array Int Int) (! a :next a_next))
(declare-fun b () (Array Int Int))
(declare-fun b_next () (Array Int Int))
(define-fun .b () (Array Int Int) (! b :next b_next))
(declare-fun c () (Array Int Int))
(declare-fun c_next () (Array Int Int))
(define-fun .c () (Array Int Int) (! c :next c_next))
(declare-fun d () (Array Int Int))
(declare-fun d_next () (Array Int Int))
(define-fun .d () (Array Int Int) (! d :next d_next))
(declare-fun i () Int)
(declare-fun i_next () Int)
(define-fun .i () Int (! i :next i_next))
(declare-fun N () Int)
(declare-fun N_next () Int)
(define-fun .N () Int (! N :next N_next))
(declare-fun x () Int)
(declare-fun x_next () Int)
(define-fun .x () Int (! x :next x_next))
(declare-fun pc () Int)
(declare-fun pc_next () Int)
(define-fun .pc () Int (! pc :next pc_next))
(declare-fun Z () Int)
(declare-fun Z_next () Int)
(define-fun .Z () Int (! Z :next Z_next))

(define-fun init-conditions () Bool (!
 (and
(= i 0)
(= pc 1)
) :init true))

(define-fun trans-conditions () Bool (!
 (and
(=> (and (< i N) (= pc 1)) (= (store a i x) a_next))
(=> (and (>= i N) (= pc 1)) (= a a_next))
(=> (and (< i N) (= pc 2)) (= a a_next))
(=> (and (>= i N) (= pc 2)) (= a a_next))
(=> (and (< i N) (= pc 3)) (= a a_next))
(=> (and (>= i N) (= pc 3)) (= a a_next))
(=> (and (< i N) (= pc 4)) (= a a_next))
(=> (and (= pc 4) (not (< i N))) (= a a_next))
(=> (and (< i N) (= pc 1)) (= b b_next))
(=> (and (>= i N) (= pc 1)) (= b b_next))
(=> (and (< i N) (= pc 2)) (= (store b i (select a i)) b_next))
(=> (and (>= i N) (= pc 2)) (= b b_next))
(=> (and (< i N) (= pc 3)) (= b b_next))
(=> (and (>= i N) (= pc 3)) (= b b_next))
(=> (and (< i N) (= pc 4)) (= b b_next))
(=> (and (= pc 4) (not (< i N))) (= b b_next))
(=> (and (< i N) (= pc 1)) (= c c_next))
(=> (and (>= i N) (= pc 1)) (= c c_next))
(=> (and (< i N) (= pc 2)) (= c c_next))
(=> (and (>= i N) (= pc 2)) (= c c_next))
(=> (and (< i N) (= pc 3)) (= (store c i (select b i)) c_next))
(=> (and (>= i N) (= pc 3)) (= c c_next))
(=> (and (< i N) (= pc 4)) (= c c_next))
(=> (and (= pc 4) (not (< i N))) (= c c_next))
(=> (and (< i N) (= pc 1)) (= d d_next))
(=> (and (>= i N) (= pc 1)) (= d d_next))
(=> (and (< i N) (= pc 2)) (= d d_next))
(=> (and (>= i N) (= pc 2)) (= d d_next))
(=> (and (< i N) (= pc 3)) (= d d_next))
(=> (and (>= i N) (= pc 3)) (= d d_next))
(=> (and (< i N) (= pc 4)) (= (store d i (select c i)) d_next))
(=> (and (= pc 4) (not (< i N))) (= d d_next))
(=> (and (< i N) (= pc 1)) (= (+ i 1) i_next))
(=> (and (>= i N) (= pc 1)) (= 0 i_next))
(=> (and (< i N) (= pc 2)) (= (+ i 1) i_next))
(=> (and (>= i N) (= pc 2)) (= 0 i_next))
(=> (and (< i N) (= pc 3)) (= (+ i 1) i_next))
(=> (and (>= i N) (= pc 3)) (= 0 i_next))
(=> (and (< i N) (= pc 4)) (= (+ i 1) i_next))
(=> (and (= pc 4) (not (< i N))) (= i i_next))
(= N N_next)
(=> (and (< i N) (= pc 1)) (= x x_next))
(=> (and (>= i N) (= pc 1)) (= x x_next))
(=> (and (< i N) (= pc 2)) (= x x_next))
(=> (and (>= i N) (= pc 2)) (= x x_next))
(=> (and (< i N) (= pc 3)) (= x x_next))
(=> (and (>= i N) (= pc 3)) (= x x_next))
(=> (and (< i N) (= pc 4)) (= x x_next))
(=> (and (= pc 4) (not (< i N))) (= x x_next))
(=> (and (< i N) (= pc 1)) (= 1 pc_next))
(=> (and (>= i N) (= pc 1)) (= 2 pc_next))
(=> (and (< i N) (= pc 2)) (= 2 pc_next))
(=> (and (>= i N) (= pc 2)) (= 3 pc_next))
(=> (and (< i N) (= pc 3)) (= 3 pc_next))
(=> (and (>= i N) (= pc 3)) (= 4 pc_next))
(=> (and (< i N) (= pc 4)) (= 4 pc_next))
(=> (and (= pc 4) (not (< i N))) (= pc pc_next))
(= Z Z_next)
(or (= pc 1) (= pc 2) (= pc 3) (= pc 4))
) :trans true))

(define-fun property () Bool (!
 (and
(let ((a!1 (not (not (= (select d Z) (select a Z))))))
  (=> (and (= pc 4) (>= i N) (>= Z 0) (< Z N)) (and a!1)))
) :invar-property 0))

