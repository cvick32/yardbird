(declare-fun a () (Array Int Int))
(declare-fun a_next () (Array Int Int))
(define-fun .a () (Array Int Int) (! a :next a_next))
(declare-fun i () Int)
(declare-fun i_next () Int)
(define-fun .i () Int (! i :next i_next))
(declare-fun j () Int)
(declare-fun j_next () Int)
(define-fun .j () Int (! j :next j_next))
(declare-fun N () Int)
(declare-fun N_next () Int)
(define-fun .N () Int (! N :next N_next))
(declare-fun pc () Int)
(declare-fun pc_next () Int)
(define-fun .pc () Int (! pc :next pc_next))
(declare-fun ZZ () Int)
(declare-fun ZZ_next () Int)
(define-fun .ZZ () Int (! ZZ :next ZZ_next))
(declare-fun Z () Int)
(declare-fun Z_next () Int)
(define-fun .Z () Int (! Z :next Z_next))

(define-fun init-conditions () Bool (!
 (and
(= i 0)
(= pc 1)
) :init true))

(define-fun trans-conditions () Bool (!
 (and
(=> (and (< i N) (= pc 1)) (= a a_next))
(let ((a!1 (= (store (store a i (select a j)) j (select a i)) a_next)))
  (=> (and (> (select a i) (select a j)) (< j N) (= pc 2)) a!1))
(let ((a!1 (and (not (> (select a i) (select a j))) (< j N) (= pc 2))))
  (=> a!1 (= a a_next)))
(=> (and (not (< j N)) (= pc 2)) (= a a_next))
(let ((a!1 (and (= pc 2) (not (not (< j N))))))
  (=> a!1 (= a a_next)))
(=> (and (< i N) (= pc 1)) (= (+ i 1) i_next))
(=> (and (< j N) (= pc 2)) (= i i_next))
(=> (and (not (< j N)) (= pc 2)) (= (+ i 1) i_next))
(let ((a!1 (and (= pc 2) (not (not (< j N))))))
  (=> a!1 (= i i_next)))
(=> (and (< i N) (= pc 1)) (= j j_next))
(=> (and (< j N) (= pc 2)) (= (+ j 1) j_next))
(=> (and (not (< j N)) (= pc 2)) (= j j_next))
(let ((a!1 (and (= pc 2) (not (not (< j N))))))
  (=> a!1 (= j j_next)))
(= N N_next)
(=> (and (< i N) (= pc 1)) (= 2 pc_next))
(=> (and (< j N) (= pc 2)) (= 2 pc_next))
(=> (and (not (< j N)) (= pc 2)) (= 1 pc_next))
(let ((a!1 (and (= pc 2) (not (not (< j N))))))
  (=> a!1 (= pc pc_next)))
(=> (and (< i N) (= pc 1)) (= ZZ ZZ_next))
(=> (and (< j N) (= pc 2)) (= ZZ ZZ_next))
(=> (and (not (< j N)) (= pc 2)) (= ZZ ZZ_next))
(let ((a!1 (and (= pc 2) (not (not (< j N))))))
  (=> a!1 (= ZZ ZZ_next)))
(= Z Z_next)
(or (= pc 1) (= pc 2))
) :trans true))

(define-fun property () Bool (!
 (and
(let ((a!1 (not (not (<= (select a Z) (select a Z))))))
  (=> (and (= pc 1) (not (< i N)) (> Z 0) (< Z N) (< Z Z) (< Z N)) (and a!1)))
) :invar-property 0))

